<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:f="sap.ui.layout.form" 
	displayBlock="true"
    controllerName="Jepco.ISU.DM.smartmeter.controller.NewRep"
  
> 
	<VBox  class="sapUiSmallMargin">
		<f:SimpleForm   id="SimpleFormStandReportFormn"
                        editable="true"
                        layout="ColumnLayout"
                        title= 'تقرير بالعدادات الجديدة غير المحدثة'
                        columnsM="2"
                        columnsL="3"
                        columnsXL="3">
 
  		</f:SimpleForm>	
		 
		<f:SimpleForm   id="SimpleFormStandReport"
                        editable="true"
                        layout="ColumnLayout"
                        columnsM="2"
                        columnsL="3"
                        columnsXL="3">
			<f:content>
			    <Button text="Export Excel File"  width="200px" press="onStandRepExcelBTNPressNew"
					 >
					<layoutData>
					<f:ColumnElementData  cellsSmall="2" cellsLarge="2" />
						 <FlexItemData growFactor="2" />  
					</layoutData>
				</Button>
			</f:content>
        </f:SimpleForm>	
	 
   </VBox>
</mvc:View>