<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified">
    <l:VerticalLayout class="sapUiMediumMarginEnd">
        <RadioButtonGroup id="rbgAssignMru" columns="4" class="sapUiLargMarginEnd" select="onShowButton1">
            <RadioButton id="RDBTour" text="حسب الجوله" />
            <RadioButton id="RDBSingle" text="عدادات إفرادي" />
            <RadioButton id="RDBFileExecl" text="ملف Excel" />
            <RadioButton id="RDBTourSingleMeter" text="إختيار عدادات من جوله" />
        </RadioButtonGroup>
        <f:SimpleForm
            visible="true"
            id="SimpleFormChange354wideDual"
            editable="true"
            layout="ResponsiveGridLayout"
            title="حسب الجوله"
            labelSpanXL="4"
            labelSpanL="3"
            labelSpanM="4"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="4"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <Label text="اسم المستخدم للفرقة" labelFor="teamName" />
            <Select id="teamName" width="390px" columnRatio="1:3" showSecondaryValues="true" items="{ path: 'userModel>/items' }">
                <core:ListItem text="{userModel>firstColumnText}" additionalText="{userModel>secondColumnText}" />
            </Select>
            <Label text="الجولة" labelFor="teamRound" />
            <Input id="teamRound" width="390px" />
            <Label text="" />
            <FlexBox height="100px" alignItems="Center" justifyContent="Start">
                <Button text="حفظ" width="85px" press="onSaveRound" class="sapUiSmallMarginEnd" />
                <Button type="Accept" width="85px" press="onQueryRound" text="استعلام" class="sapUiSmallMarginEnd" />
                <Button type="Reject" width="85px" press="onDeleteRound" text="حــذف" class="sapUiSmallMarginEnd" />
                <Button text="عرض الجميع" width="85px" press="onShowAll" />
            </FlexBox>
        </f:SimpleForm>
        <f:SimpleForm
            id="singleMeter"
            editable="true"
            layout="ResponsiveGridLayout"
            title=" عدادات افرادي "
            visible="false"
            labelSpanXL="4"
            labelSpanL="3"
            labelSpanM="4"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="4"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <Label text="اسم المستخدم " labelFor="teamName1" />
            <Select id="teamName1" width="390px" columnRatio="1:3" showSecondaryValues="true" items="{ path: 'userModel>/items' }">
                <core:ListItem text="{userModel>firstColumnText}" additionalText="{userModel>secondColumnText}" />
            </Select>
            <Label text=" رقم العداد " labelFor="numMeter" />
            <Input id="numMeter" type="Number" width="390px" columnRatio="1:3" showSecondaryValues="true" />
            <Label text="" />
            <Panel>
                <FlexBox height="100px" alignItems="Center" justifyContent="Center">
                    <Button text="حفظ" press="saveSingleMeter" design="Emphasized" width="100px" height="20px" />
                    <Button text="اضافه" press="addSingleMeter" design="Emphasized" width="100px" height="20px" />
                </FlexBox>
            </Panel>
        </f:SimpleForm>
        <f:SimpleForm
            id="execlFile"
            editable="false"
            layout="ResponsiveGridLayout"
            title=" ملف Excel"
            visible="false"
            labelSpanXL="4"
            labelSpanL="3"
            labelSpanM="4"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="4"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <Label text="اسم المستخدم " labelFor="teamName2" />
            <Select id="teamName2" width="390px" columnRatio="1:3" showSecondaryValues="true" items="{ path: 'userModel>/items' }">
                <core:ListItem text="{userModel>firstColumnText}" additionalText="{userModel>secondColumnText}" />
            </Select>
            <Label text=" ملف Excel" labelFor="execlAttacment" />
            <u:FileUploader id="fileUploader" name="myFileUpload" uploadUrl="upload/" tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" />
            <Label text="" />

            <Panel>
                <FlexBox height="100px" alignItems="Center" justifyContent="Center">
                    <Button text="حفظ" press="saveExcelFile" design="Emphasized" width="100px" height="20px" />
                    <Button text="اضافه" press="addExcelFile" design="Emphasized" width="100px" height="20px" />
                </FlexBox>
            </Panel>
        </f:SimpleForm>
        <f:SimpleForm
            id="singleMeterTour"
            editable="true"
            layout="ResponsiveGridLayout"
            title="اختيار عدادات من جوله"
            visible="false"
            labelSpanXL="4"
            labelSpanL="3"
            labelSpanM="4"
            labelSpanS="12"
            adjustLabelSpan="false"
            emptySpanXL="0"
            emptySpanL="4"
            emptySpanM="0"
            emptySpanS="0"
            columnsXL="2"
            columnsL="1"
            columnsM="1"
            singleContainerFullSize="false"
        >
            <Label text="اسم المستخدم " labelFor="teamName3" class="margin" />
            <Select id="teamName3" width="390px" columnRatio="1:3" showSecondaryValues="true" items="{ path: 'userModel>/items' }">
                <core:ListItem text="{userModel>firstColumnText}" additionalText="{userModel>secondColumnText}" />
            </Select>
            <Label text=" رقم الجوله" labelFor="singleMeterInTour" />
            <Input id="singleMeterInTour" class="margin" type="Number" width="390px" columnRatio="1:3" showSecondaryValues="true" />
            <Label text="" />

            <Panel>
                <FlexBox height="100px" alignItems="Center" justifyContent="Center">
                    <Button text="حفظ" press="saveSingleMeterTour" design="Emphasized" width="100px" height="20px" />
                    <Button text="اضافه" press="addSingleMeterTour" design="Emphasized" width="100px" height="20px" />
                </FlexBox>
            </Panel>
        </f:SimpleForm>
        <Table id="idTableRound" visible="true" items="{
			path: '/d/results',
			sorter: {
				path: 'Name'
			}
		}">
            <headerToolbar>
                <OverflowToolbar>
                    <Title text="الجولات" level="H2" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>

            <columns>
                <Column>
                    <Text text="الفرقة الفنية" />
                </Column>
                <Column>
                    <Text text="الجولة" />
                </Column>
                <Column>
                    <Text text="عدد العدادات" />
                </Column>
                <Column>
                    <Text text="تم التبديل" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle" type="Navigation">
                    <cells>
                        <Text text="{geraet}" wrapping="false" />
                        <Text text="{customeName}" wrapping="false" />
                        <Text text="{duplicateAddr}" wrapping="false" />
                        <Text text="{lequnr}" wrapping="false" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table id="tableSingleMeter" visible="false" items="{dataInput>/}"   >
            <headerToolbar>
                <OverflowToolbar>
                    <Title text="عدادات فرادي" level="H2" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <columns>
                <Column>
                    <Text text="رقم العداد" />
                </Column>
                <Column>
                    <Text text="اسم الفني" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle" type="Navigation">
                    <cells>
                        <Text text="{dataInput>number}" wrapping="false" />
                        <Text text="{dataInput>name}" wrapping="false" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table id="tableFileExcel" visible="false" >
            <headerToolbar>
                <OverflowToolbar>
                    <Title text="ملف Excel" level="H2" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <columns>
                <Column>
                    <Text text="رقم العداد" />
                </Column>
                <Column>
                    <Text text="اسم الفني" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle" type="Navigation">
                    <cells>
                        <Text text="2222" wrapping="false" />
                        <Text text="وسام عيسى حسن احمد" wrapping="false" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <Table id="tableSingleMeterTour" visible="false">
            <headerToolbar>
                <OverflowToolbar>
                    <Title text="اختيار عدادات من جوله" level="H2" />
                    <ToolbarSpacer />
                </OverflowToolbar>
            </headerToolbar>
            <columns>
                <Column>
                    <Text text="رقم العداد" />
                </Column>
                <Column>
                    <Text text="اسم الفني" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle" type="Navigation">
                    <cells>
                        <Text text="2222" wrapping="false" />
                        <Text text="وسام عيسى حسن احمد" wrapping="false" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </l:VerticalLayout>
</core:FragmentDefinition>
